@model List<StockItem>
@{
    ViewData["Title"] = "Home Page";
}

<head>
    <script src="https://cdn.plot.ly/plotly-latest.min.js"></script>
    <script type="text/javascript" src="~/js/plotting.js"></script>
</head>
<body>
    <div class="container">
        <div class="text-block" style="text-align:center">
            @foreach (var item in Model)
            {
               <a class="bg-info" asp-controller="Home" asp-action="Analyze" asp-route-Id="@item.Id">@item.TodaysDate</a>
            }
        </div>
        <div id="tester" style="align-content:center"></div>
        <script>
            TESTER = document.getElementById('tester');
            Plotly.plot(TESTER, [{
                x: [1, 2, 3, 4, 5],
                y: [1, 2, 4, 8, 16]
            }], {
                    margin: { t: 0 }
                });
        </script>
    </div>
</body>



